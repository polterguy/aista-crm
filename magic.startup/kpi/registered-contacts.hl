
/*
 * KPI file for registered contacts.
 */
.name:Latest contacts
.description:5 last created contacts
.type:table
.lambda
   data.connect:[generic|aista-crm]
      data.read
         table:contacts
         columns
            name
            email
            created
         order:created
         direction:desc
         limit:5
      return-nodes:x:-/*
