
/*
 * KPI file for displaying funnel information.
 */
.name:Employee productivity
.description:Closed activities by user for current month
.type:pie
.lambda
   data.connect:[generic|aista-crm]
      date.now
      date.format:x:-
         format:yyyy-MM-dd
      data.select:@"
select username as name, count(*) as value
   from activities
      where closed > @date and done = 1
   group by username"
         @date:x:@date.format
      return-nodes:x:-/*
